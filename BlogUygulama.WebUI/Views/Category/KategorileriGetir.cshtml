@model List<BlogUygulama.Entities.Concrete.Category>

@foreach (var kategoriler in Model)
{
    if (kategoriler.KategoriAdi != null)
    {
        <div class="card my-4">
            <h5 class="card-header">Kategoriler</h5>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-6">
                        <ul class="list-unstyled mb-0">
                            <li>
                                <a href="#">@kategoriler.KategoriAdi</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    }
}